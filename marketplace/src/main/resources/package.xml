<package type="addon" name="nuxeo-diff" version="@VERSION@">
  <title>Nuxeo Diff</title>
  <description>The Nuxeo Diff addon enables to compare two documents or two versions of a document to see the differences between documents or versions, including metadata values comparison, as well as office files.
  </description>
  <home-page>https://doc.nuxeo.com/nxdoc/nuxeo-diff/</home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <!-- <hotreload-support>false</hotreload-support> -->
  <!-- <require-terms-and-conditions-acceptance>false -->
  <!-- </require-terms-and-conditions-acceptance> -->
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>production_ready</production-state>
  <supported>true</supported>
  <platforms>
    <platform>server-@DISTRIB_VERSION@</platform>
    <platform>server-@DISTRIB_VERSION@-*</platform>
  </platforms>
  <dependencies>
    <package>nuxeo-jsf-ui</package>
  </dependencies>
  <license>LGPL</license>
  <license-url>http://www.gnu.org/licenses/lgpl.html</license-url>
  <visibility>PUBLIC</visibility>
</package>
